<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="false" />
	<package name="praticeProject" extends="struts-default">
		<action name='index' class="com.pratice.action.StudentAction">
			<result>/WEB-INF/views/index.jsp</result>
		</action>
		<action name="helloword" class="com.pratice.action.Test" method="test">
			<result name="success">/WEB-INF/views/success.jsp</result>
		</action>
		<action name="login" class="com.pratice.action.UserAction" method="login">
			<result>/WEB-INF/views/index.jsp</result>
			<result name="error">/login.jsp</result>
		</action>
		<action name="toIntention" class="com.pratice.action.StudentAction" method="toIntent">
			<result>/WEB-INF/views/intention.jsp</result>
		</action>
		<action name="intention" class="com.pratice.action.StudentAction" method="intention">
			<result>/WEB-INF/views/index.jsp</result>
			<result name="error">/WEB-INF/views/intention.jsp</result>
		</action>
		<action name="toPractice" class="com.pratice.action.StudentAction" method="toPractice">
			<result>/WEB-INF/views/stupractice.jsp</result>
		</action>
		<action name="practice" class="com.pratice.action.StudentAction" method="practice">
			<result>/WEB-INF/views/index.jsp</result>
			<result name="error">/WEB-INF/views/stupractice.jsp</result>
		</action>
		<action name="toDiary" class="com.pratice.action.StudentAction" method="toDiary">
			<result>/WEB-INF/views/diary.jsp</result>
		</action>
		<action name="toAddDiary" class="com.pratice.action.StudentAction" method="toAddDiary">
			<result>/WEB-INF/views/addDiary.jsp</result>
		</action>
		<action name="addDiary" class="com.pratice.action.StudentAction" method="addDiary">
			<result type="redirectAction">
				<param name="actionName">toDiary</param>
			</result>
		</action>
		<action name="showDiary" class="com.pratice.action.StudentAction" method="showDiary">
			<result>/WEB-INF/views/showDiary.jsp</result>
		</action>
		<action name="updateDiary" class="com.pratice.action.StudentAction" method="updateDiary">
			<result type="redirectAction">
				<param name="actionName">toDiary</param>
			</result>
		</action>
		<action name="toReport" class="com.pratice.action.StudentAction" method="toReport">
			<result>/WEB-INF/views/stuReport.jsp</result>
		</action>
		<action name="toSummary" class="com.pratice.action.StudentAction" method="toSummary">
			<result>/WEB-INF/views/stuSummary.jsp</result>
		</action>
		<action name="stuReport" class="com.pratice.action.StudentAction" method="stuReport">
			<result>/WEB-INF/views/index.jsp</result>
		</action>
		<action name="stuSummary" class="com.pratice.action.StudentAction" method="stuSummary">
			<result>/WEB-INF/views/index.jsp</result>
		</action>
		<action name="allMessages" class="com.pratice.action.MessageAction" method="listAllMessage">
			<result>/WEB-INF/views/msglist.jsp</result>
		</action>
		<action name="msgContent" class="com.pratice.action.MessageAction" method="getMessageContent">
			<result>/WEB-INF/views/content.jsp</result>
		</action>
		<action name="msgCommit" class="com.pratice.action.MessageAction" method="pushMsg">
			<result>/WEB-INF/views/success.jsp</result>
		</action>
		<action name="fileDownFile" class="com.pratice.action.FileAction" method="fileDown">
			<result type="stream">
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">attachment;filename=${downloadFileName}</param>
			</result>
		</action>
		
		<action name="insertInfo" class="com.pratice.action.InsertInfoAction" method="insertInfo">
			<result name="success">/WEB-INF/views/success.jsp</result>
		</action>
		<action name="messageform" class="com.pratice.action.MessageAction">
			<result>/WEB-INF/views/messageform.jsp</result>
		</action>
		<action name="upload" class="com.pratice.action.InsertInfoAction">
			<result>/WEB-INF/views/upload.jsp</result>
		</action>
		<action name="userInforPage" class="com.pratice.action.UserInfoAction">
			<result>/WEB-INF/views/queryUserInfo.jsp</result>
		</action>
		<action name="teachersInfoTable" class="com.pratice.action.TeacherInfoAction">
			<result>/WEB-INF/views/table.jsp</result>
		</action>
		<action name="studentsInfoTable" class="com.pratice.action.StudentInfoAction">
			<result>/WEB-INF/views/table.jsp</result>
		</action>
		<action name="exportExcel" class="com.pratice.action.FileAction" method="excelExport">
			<result type="stream">
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">attachment;filename=${downloadFileName}</param>
			</result>
		</action>
		<action name="exportFilePage" class="com.pratice.action.FileAction" method="exportFilePage">
			<result>/WEB-INF/views/user_info_export.jsp</result>
		</action>
		<action name="deleteMessage" class="com.pratice.action.MessageAction" method="deleteMessage">
			<interceptor-ref name="paramsPrepareParamsStack"></interceptor-ref>
			<result>/WEB-INF/views/success.jsp</result>
		</action>
		<action name="getEditForm" class="com.pratice.action.MessageAction" method="getEditForm">
			<interceptor-ref name="paramsPrepareParamsStack"></interceptor-ref>
			<result>/WEB-INF/views/editMessageForm.jsp</result>
		</action>
		<action name="updateMessage" class="com.pratice.action.MessageAction" method="updateMessage">
			<interceptor-ref name="paramsPrepareParamsStack"></interceptor-ref>
			<result>/WEB-INF/views/success.jsp</result>
		</action>
	</package>
	
	
	<package name="testJson" extends="json-default">
		<action name="studentsInfo" class="com.pratice.action.StudentInfoAction" method="sudentsInfo">
			<result type="json">
				<param name="root">jsonBean</param>
			</result>
		</action>
		<action name="teachersInfo" class="com.pratice.action.TeacherInfoAction" method="teachersInfo">
			<result type="json">
				<param name="root">jsonBean</param>
			</result>
		</action>
		<action name="notNullTeacher" class="com.pratice.action.TeacherInfoAction" method="getNotFullTeacher">
			<result type="json">
				<param name="root">jsonTecher</param>
			</result>
		</action>
		<action name="updateStudentTutor" class="com.pratice.action.StudentInfoAction" method="updateStudentTutor">
			<interceptor-ref name="paramsPrepareParamsStack"></interceptor-ref>
			<result type="json">
				<param name="root">list</param>
			</result>
		</action>
	</package>
	
</struts>    
